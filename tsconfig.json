{
  "compilerOptions": {
    "lib": ["es2016", "esnext.asynciterable","DOM"],
    // "target": "es6",
    // "module": "commonjs",
    "target": "ESNext",
    "module": "commonjs",
    "esModuleInterop": true,
    "outDir": "./dist",
        // "esModuleInterop": true,
    // "outFile": "./dist/main.js", // Specify the output file with .js extension
    "strict": true,
    "noErrorTruncation":true,
    "noUnusedLocals": true,
    "noImplicitAny": true,
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"]
}
